{
  "id": "data-platform-crew-008",
  "name": "Data Platform Crew",
  "description": "Team for building comprehensive data platforms and analytics",
  "goal": "Build scalable data infrastructure for analytics and business intelligence",
  "agents": [
    "data-engineer",
    "database-architect",
    "data-scientist",
    "backend-python-developer",
    "devops-engineer"
  ],
  "process": "sequential",
  "verbose": true,
  "tasks": [
    {
      "name": "data_warehouse_design",
      "description": "Design data warehouse architecture",
      "agent": "database-architect",
      "expected_output": "Data warehouse schema and architecture"
    },
    {
      "name": "etl_pipeline",
      "description": "Build ETL/ELT pipelines",
      "agent": "data-engineer",
      "expected_output": "Data pipelines with orchestration"
    },
    {
      "name": "analytics_layer",
      "description": "Create analytics and reporting layer",
      "agent": "data-scientist",
      "expected_output": "Analytics models and dashboards"
    },
    {
      "name": "api_layer",
      "description": "Build data API layer",
      "agent": "backend-python-developer",
      "expected_output": "APIs for data access and queries"
    },
    {
      "name": "platform_deployment",
      "description": "Deploy and maintain data platform",
      "agent": "devops-engineer",
      "expected_output": "Automated deployment and monitoring"
    }
  ],
  "manager_agent": "data-engineer",
  "max_rpm": 80,
  "language": "en",
  "full_output": true
}
