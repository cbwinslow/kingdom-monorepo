{
  "id": "devops-automation-crew-010",
  "name": "DevOps Automation Crew",
  "description": "Team focused on automating development and operations workflows",
  "goal": "Implement comprehensive DevOps practices and automation",
  "agents": [
    "devops-engineer",
    "ci-cd-specialist",
    "kubernetes-specialist",
    "site-reliability-engineer",
    "observability-engineer"
  ],
  "process": "sequential",
  "verbose": true,
  "tasks": [
    {
      "name": "ci_cd_pipeline",
      "description": "Build CI/CD pipelines",
      "agent": "ci-cd-specialist",
      "expected_output": "Automated build, test, and deployment pipelines"
    },
    {
      "name": "infrastructure_automation",
      "description": "Automate infrastructure provisioning",
      "agent": "devops-engineer",
      "expected_output": "Infrastructure as Code with Terraform"
    },
    {
      "name": "kubernetes_setup",
      "description": "Setup Kubernetes orchestration",
      "agent": "kubernetes-specialist",
      "expected_output": "Kubernetes cluster with deployments"
    },
    {
      "name": "observability",
      "description": "Implement comprehensive observability",
      "agent": "observability-engineer",
      "expected_output": "Metrics, logs, and traces collection"
    },
    {
      "name": "reliability",
      "description": "Ensure system reliability",
      "agent": "site-reliability-engineer",
      "expected_output": "SLOs, alerting, and incident response"
    }
  ],
  "manager_agent": "devops-engineer",
  "max_rpm": 100,
  "language": "en",
  "full_output": true
}
