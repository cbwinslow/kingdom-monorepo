  "agents": [
    "game-developer",
    "graphics-programmer",
    "backend-api-developer",
    "ui-ux-designer",
    "qa-automation-engineer"
  ],
  "process": "sequential",
  "verbose": true,
  "tasks": [
    {
      "name": "game_design",
      "description": "Design game mechanics and UI",
      "agent": "ui-ux-designer",
