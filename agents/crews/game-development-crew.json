      "expected_output": "Rendering engine and shaders"
    },
    {
      "name": "gameplay_programming",
      "description": "Implement gameplay mechanics",
      "agent": "game-developer",
      "expected_output": "Core gameplay systems"
    },
    {
      "name": "backend_services",
      "description": "Build multiplayer and backend services",
      "agent": "backend-api-developer",
      "expected_output": "Game servers and APIs"
    },
    {
      "name": "quality_assurance",
      "description": "Test game functionality",
      "agent": "qa-automation-engineer",
      "expected_output": "Game testing and bug reports"
    }
  ],
  "manager_agent": "game-developer",
  "max_rpm": 100,
  "language": "en",
  "full_output": true
