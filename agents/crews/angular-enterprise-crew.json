{
  "id": "angular-enterprise-crew-037",
  "name": "Angular Enterprise Application Crew",
  "description": "Team for building large-scale Angular enterprise applications",
  "goal": "Develop maintainable, scalable Angular applications for enterprise use",
  "agents": [
    "frontend-angular-developer",
    "ui-ux-designer",
    "backend-api-developer",
    "system-architect",
    "qa-automation-engineer"
  ],
  "process": "sequential",
  "verbose": true,
  "tasks": [
    {
      "name": "architecture_planning",
      "description": "Plan Angular architecture",
      "agent": "system-architect",
      "expected_output": "Angular application architecture"
    },
    {
      "name": "design_system",
      "description": "Create design system",
      "agent": "ui-ux-designer",
      "expected_output": "Enterprise design system"
    },
    {
      "name": "angular_development",
      "description": "Develop Angular application",
      "agent": "frontend-angular-developer",
      "expected_output": "Angular enterprise application"
    },
    {
      "name": "backend_services",
      "description": "Build backend services",
      "agent": "backend-api-developer",
      "expected_output": "Enterprise APIs with security"
    },
    {
      "name": "comprehensive_testing",
      "description": "Test Angular application",
      "agent": "qa-automation-engineer",
      "expected_output": "Unit, integration, and E2E tests"
    }
  ],
  "manager_agent": "system-architect",
  "max_rpm": 100,
  "language": "en",
  "full_output": true
}
