# WireGuard Server Configuration
SERVERURL=auto  # Or your domain: vpn.example.com
SERVERPORT=51820
PEERS=10  # Number of client configurations to generate
PEERDNS=auto  # Or specific DNS: 1.1.1.1
INTERNAL_SUBNET=10.13.13.0
ALLOWEDIPS=0.0.0.0/0  # Or specific subnets: 10.0.0.0/8,192.168.0.0/16

# WireGuard UI
SESSION_SECRET=change_this_to_random_string
WGUI_USERNAME=admin
WGUI_PASSWORD=change_this_secure_password

# OpenVPN Admin
OVPN_ADMIN_USER=admin
OVPN_ADMIN_PASS=change_this_secure_password

# IPsec VPN
IPSEC_PSK=change_this_pre_shared_key
IPSEC_USER=vpnuser
IPSEC_PASSWORD=change_this_secure_password

# SoftEther VPN
SOFTETHER_PSK=change_this_pre_shared_key
SOFTETHER_SPW=change_this_server_password
SOFTETHER_HPW=change_this_hub_password

# Tailscale
TS_AUTHKEY=your_tailscale_auth_key

# Pi-hole
PIHOLE_PASSWORD=change_this_secure_password
SERVERIP=10.13.13.1
